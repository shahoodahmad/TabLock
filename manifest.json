{
  "name": "TabLock",
  "version": "1.0",
  "description": "Securely save your tabs and windows",
  "permissions": ["tabs", "storage", "windows"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "16": "favicon.png",
      "32": "favicon.png",
      "48": "favicon.png",
      "128": "favicon.png"
    },
    "default_popup": "instance.html"
  },
  "manifest_version": 2


}
