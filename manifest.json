{
  "name": "TabLock",
  "version": "1.0",
  "description": "Temporarily lock tabs for privacy",
  "permissions": ["tabs", "history", "storage", "windows"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "16": "favicon.png",
      "32": "favicon.png",
      "48": "favicon.png",
      "128": "favicon.png"
    },
    "default_popup": "instance.html"
  },
  "options_page": "options.html",
  "manifest_version": 2


}
